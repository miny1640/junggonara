@charset "EUC-KR";



#btn-raised{
  background:#1AAB8A;
  color:#fff;
  border:none;
  position:absolute;
  height:80px;
  font-size:10px;
  padding:0 2em;
  cursor:pointer;
  transition:800ms ease all;
  outline:none;
}
#btn-raised:hover{
  background:#fff;
  color:#1AAB8A;
}
#btn-raised:before, #btn-raised:after{
  content:'';
  position:absolute;
  top:0;
  right:0;
  height:2px;
  width:0;
  background: #1AAB8A;
  transition:400ms ease all;
}
#btn-raised:after{
  right:inherit;
  top:inherit;
  left:0;
  bottom:0;
}
#btn-raised:hover:before, #btn-raised:hover:after{
  width:100%;
  transition:800ms ease all;

}


@import url(https://fonts.googleapis.com/css?family=BenchNine:700);

.snip1535 {
  background-color:#8181F7;
  border: none;
  color: #ffffff;
  cursor: pointer;
  display: inline-block;
  font-family: 'BenchNine', Arial, sans-serif;
  font-size: 1em;
  font-size: 8px;
  line-height: 1em;
  margin: 5px 5px;
  outline: none;
  padding: 12px 40px 10px;
  position: relative;
  text-transform: uppercase;
  font-weight: 700;
}
.snip1535:before,
.snip1535:after {
  border-color: transparent;
  -webkit-transition: all 0.25s;
  transition: all 0.25s;
  border-style: solid;
  border-width: 0;
  content: "";
  height: 24px;
  position: absolute;
  width: 24px;
}
.snip1535:before {
  border-color: #8181F7;
  border-right-width: 2px;
  border-top-width: 2px;
  right: -5px;
  top: -5px;
}
.snip1535:after {
  border-bottom-width: 2px;
  border-color:#8181F7;
  border-left-width: 2px;
  bottom: -5px;
  left: -5px;
}
.snip1535:hover,
.snip1535.hover {
  background-color: #0000FF;
}
.snip1535:hover:before,
.snip1535.hover:before,
.snip1535:hover:after,
.snip1535.hover:after {
  height: 100%;
  width: 100%;
}
