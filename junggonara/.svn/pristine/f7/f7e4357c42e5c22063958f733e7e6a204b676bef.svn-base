@charset "EUC-KR"; 

/*서브(리스트)_페이지넘버 CSS*/
.list_number{padding-top:25px; text-align:center;}
.list_n_menu {padding:3px; MARGIN:3px; TEXT-ALIGN: center}
.list_n_menu A {padding:5px 8px 4px 8px; MARGIN: 2px; BORDER: #ccc 1px solid; COLOR: #000000; TEXT-DECORATION: none}
.list_n_menu A:hover {BORDER:#0000001px solid; COLOR: #000000;}
.list_n_menu A:active {BORDER:#000000 1px solid; COLOR: #000000;}
.list_n_menu .current {BORDER: #000000 1px solid; padding:5px 8px 4px 8px; FONT-WEIGHT: bold; MARGIN: 2px; COLOR: #000000;}
.list_n_menu .disabled {BORDER: #eee 1px solid; padding:5px 8px 4px 8px; MARGIN: 2px; COLOR: #000000;}
    
/*detail*/
#detail{
	border:2px solid blue;
}

/*패스워드 서치 폼*/
#passwordfield{
	border-bottom-width:7px;
	border-right-width:7px;
	border-right-color:lightgrey;
	border-bottom-color:lightgrey;
}
    